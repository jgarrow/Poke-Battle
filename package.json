{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "@react-hook/window-scroll": "^1.0.7",
    "apollo-boost": "^0.4.4",
    "gatsby": "^2.18.11",
    "gatsby-image": "^2.2.36",
    "gatsby-plugin-emotion": "^4.1.18",
    "gatsby-plugin-sharp": "^2.3.7",
    "gatsby-plugin-transition-link": "^1.17.5",
    "gatsby-source-filesystem": "^2.1.42",
    "gatsby-transformer-sharp": "^2.3.9",
    "gatsby-transformer-yaml": "^2.2.20",
    "gsap": "^3.0.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-progressbar.js": "^0.2.0",
    "typeface-heebo": "0.0.72"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
